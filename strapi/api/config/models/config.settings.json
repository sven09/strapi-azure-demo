{
  "kind": "singleType",
  "collectionName": "configs",
  "info": {
    "name": "config",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "eventTitle": {
      "type": "string"
    },
    "privacyPolicyUrl": {
      "type": "string"
    },
    "imprintUrl": {
      "type": "string"
    },
    "supportEmail": {
      "type": "string"
    },
    "homepageUrl": {
      "type": "string"
    },
    "startDate": {
      "type": "datetime"
    },
    "endDate": {
      "type": "datetime"
    },
    "language": {
      "type": "enumeration",
      "enum": [
        "german",
        "english"
      ]
    },
    "defaultLayout": {
      "type": "enumeration",
      "enum": [
        "standart",
        "deck",
        "archived",
        "mediaLibrary"
      ]
    },
    "css": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
